{
  "name": "tree-sitter-alpinejs",
  "version": "0.1.0",
  "description": "AlpineJs grammar for tree-sitter",
  "repository": "https://github.com/a909m/tree-sitter-alpinejs",
  "license": "MIT",
  "author": {
    "name": "Assem Alwaseai",
    "email": "assemcc2022@gmail.com",
    "url": "https://github.com/A909M"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "alpinejs"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4",
    "tree-sitter-html": "^0.23.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.8"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "tree-sitter": [
    {
      "scope": "text.html.alpinejs",
      "file-types": [
        "html"
      ]
    }
  ]
}
